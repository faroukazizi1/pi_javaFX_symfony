<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="GroupSuccess.esprit.Main"
            prefHeight="600.0" prefWidth="800.0">

    <TabPane prefWidth="800.0" prefHeight="600.0">

        <Tab text="Congé">
            <VBox>
                <fx:include source="AjouterConge.fxml"/>
            </VBox>
        </Tab>

        <Tab text="Bulletin de Paie">
            <VBox>
                <fx:include source="BulletinPaie.fxml"/>
            </VBox>
        </Tab>

        <Tab text="Traitement de demandes">
            <VBox>
                <fx:include source="Traitement.fxml"/>
            </VBox>
        </Tab>

        <Tab text="Création Bulletin de Paie">
            <VBox>
                <fx:include source="CreationBulletin.fxml"/>
            </VBox>
        </Tab>

    </TabPane>

</AnchorPane>
