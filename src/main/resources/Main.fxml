        </Tab>

        <Tab fx:id="tabBulletinPaie" text="Bulletin de Paie">
            <VBox>
                <fx:include source="/BulletinPaie.fxml" />
            </VBox>
        </Tab>

        <Tab fx:id="tabTraitement" text="Traitement de demandes">
            <VBox>
                <fx:include source="/Traitement.fxml" />
            </VBox>
        </Tab>

        <Tab fx:id="tabCreationBulletin" text="Création Bulletin de Paie">
